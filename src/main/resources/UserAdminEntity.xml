<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demoTTS2.Model.UserAdminEntity">
        <table name="user_admin" schema="demo_ql" catalog=""/>
        <attributes>
            <id name="uaif">
                <column name="uaif" precision="-1"/>
            </id>
            <basic name="username">
                <column name="username" nullable="false" length="250"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false" length="250"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
